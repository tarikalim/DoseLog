                        "after_meal",
                        "with_meal",
                        "irrelevant"
                    ],
                    "allOf": [
                        {
                            "$ref": "#/definitions/shared.MealRelation"
                        }
                    ]
                },
                "name": {
                    "type": "string",
                    "minLength": 2
                },
                "pills_per_box": {
                    "type": "integer",
                    "minimum": 1
                },
                "strength_mg": {
                    "type": "integer"
                }
            }
        },
        "dto.MedicationLogResponse": {
            "type": "object",
